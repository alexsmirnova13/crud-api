{
  "name": "crud-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --silent",
    "test:verbose": "jest",
    "start:dev": "npx nodemon",
    "start:prod": "npm run build && node build/single/bundle.js",
    "start:multi": "npx ts-node ./src/balancer.ts",
    "start:multi:prod": "npm run build:multi && node build/multi/bundle.js",
    "build": "webpack",
    "build:multi": "webpack --config webpack.config.multi.js",
    "build:all": "npm run build && npm run build:multi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EduMGTK2020/node-crud-api.git"
  },
  "keywords": [
    "node",
    "crud"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.3",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0"
  }
}
